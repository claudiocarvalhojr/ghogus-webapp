<div id="divListagem" class="row col-xs-12">
  <h4><%= title %></h4>
  <div id="msgSave" class="alert alert-success row col-xs-12"><strong>Sucesso! </strong>Cliente cadastrado com sucesso!
  </div>
  <div id="msgDelete" class="alert alert-success row col-xs-12"><strong>Sucesso! </strong>Cliente excluído com sucesso!
  </div>
  <table cellspancig="0" class="table table-striped table-hover table-responsive table-condensed">
    <thead>
      <tr>
        <td id="colId">#</td>
        <td>Nome</td>
        <td>Idade</td>
        <td>UF</td>
        <td colspan="2">Ações</td>
      </tr>
    </thead>
    <tbody>
      <tr>
        <td colspan="6">Nenhum cliente cadastrado.</td>
      </tr>
    </tbody>
    <tfoot>
      <tr>
        <td colspan="6">
          <div class="from-group">
            <a id="btnRegister" href="/clientes/new" class="btn btn-md btn-primary">Cadastrar</a> |
            <a id="btnReturn" href="/" class="btn btn-md btn-default">Voltar</a>
          </div>
      </tr>
    </tfoot>
  </table>
</div>